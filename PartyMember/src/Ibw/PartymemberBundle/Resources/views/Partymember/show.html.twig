{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Partymember</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Stuno</th>
                <td>{{ entity.StuNo }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.Name }}</td>
            </tr>
            <tr>
                <th>Sex</th>
                <td>{{ entity.Sex }}</td>
            </tr>
            <tr>
                <th>Class</th>
                <td>{{ entity.Class }}</td>
            </tr>
            <tr>
                <th>Join_date</th>
                <td>{{ entity.joindate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Full_date</th>
                <td>{{ entity.Fulldate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Branch</th>
                <td>{{ entity.Branch }}</td>
            </tr>
            <tr>
                <th>Mobpho</th>
                <td>{{ entity.MobPho }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('ibw_partymember') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ibw_partymember_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
